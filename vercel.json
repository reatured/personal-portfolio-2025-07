{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "functions": {
    "app/**/*.{js,ts}": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "REACT_APP_SUPABASE_URL": "@supabase_url",
    "REACT_APP_SUPABASE_ANON_KEY": "@supabase_anon_key",
    "REACT_APP_PORTFOLIO_TITLE": "@portfolio_title",
    "REACT_APP_PORTFOLIO_URL": "@portfolio_url",
    "REACT_APP_PORTFOLIO_EMAIL": "@portfolio_email"
  }
}